<?xml version="1.0" encoding="UTF-8"?>

<!-- INIZIO MACRO CONTENITORE DELL'INTERFACCIA -->
<!-- INTERFACE MACRO CONTAINER START -->
<interface>

    <!-- DICHIARAZIONE DELLE DIPENDENZE DELL'INTERFACCIA -->
    <!-- DECLARING INTERFACE DEPENDENCIES -->
    <requires lib="gtk" version="4.0"/>
    <requires lib="Adw" version="1.0"/>


    <!-- APERTURA FINESTRA PRINCIPALE -->
	<!-- MAIN WINDOW START-->
    <object class="GtkWindow" id="function_1_window">

        <!-- IMPOSTAZIONE ALTEZZA ALL'APERTURA -->
		<!-- SETTING HEIGHT SIZE ON OPENING -->
        <property name="default-height">850</property>

        <!-- IMPOSTAZIONE LUNGHEZZA ALL'APERTURA -->
		<!-- SETTING WIDTH SIZE ON OPENING -->
        <property name="default-width">950</property>

        <!-- IMPOSTAZIONE DEL TITOLO NELLA BARRA DEL TITOLO -->
		<!-- SETTING THE TITLE IN THE TITLE BAR -->
        <property name="title" translatable="yes">Install Davinci Resolve dependencies</property>

		<!-- IMPOSTAZIONE DELLA FINESTRA COME FINESTRA DI DIALOGO -->
		<!-- SETTING THE WINDOW AS A DIALOG WINDOW -->
        <property name="modal">true</property>

		<!-- IMPOSTAZIONE DELLA CHISURA DELLA FINESTRA ALLA CHISURA DELLA FINESTRA PRINCIPALE -->
		<!-- SETTING THE WINDOW TO BE DESTROYED WITH HER MOTHER WINDOW -->
        <property name="destroy-with-parent">true</property>

		<!-- IMPOSTAZIONE POSSIBILITÀ DI RIDIMENSIONARE LA FINESTRA -->
		<!-- SETTING THE POSSIBILITY TO RESIZE THE WINDOW -->
        <property name="resizable">false</property>



        <!-- APERTURA CHILD GRIGLIA PRINCIPALE -->
        <!-- MAIN GRID CHILD START -->
        <child>

            <!-- APERTURA GRIGLIA PRINCIPALE -->
            <!-- MAIN GRID START -->
            <object class="GtkGrid" id="function_1_main_grid">

                <!-- SPAZIO TRA LE RIGHE -->
                <!-- ROW SPACING -->
                <property name="row-spacing">1</property>

                <!-- SPAZIO TRA LE COLONNE -->
                <!-- COLUMN SPACING -->
                <property name="column-spacing">50</property>

                <!-- PARAMETRO DI OMOGENEITÀ DELLE COLONNE -->
                <!-- SETTING COLUMN HOMOGENITY -->
                <property name="column-homogeneous">true</property>

                <!-- PARAMETRO DI OMOGENEITÀ DELLE RIGHE -->
                <!-- SETTING ROW HOMOGENITY -->
                <property name="row-homogeneous">false</property>

                <!-- INSERISCO I PARAMETRI CSS DEL TESTO TITOLO 1-->
                <!-- SETTING TITLE TEXT 1 CSS PARAMETERS -->
                <style>
                    <class name="Function-1-Grid"/>
                </style>



                <!-- APERTURA CHILD TESTO TITOLO-->
                <!-- TITLE TEXT CHILD START -->
                <child>

                    <!-- APERTURA TESTO TITOLO 1 -->
                    <!-- TITLE TEXT 1 START -->
                    <object class="GtkLabel" id="title_text_1">

                        <!-- VISIBILITÀ DELL'OGGETTO -->
                        <!-- SETTING THE OBJECT VISIBILITY -->
                        <property name="visible">False</property>
                        
                        <!-- INSERIMENTO DEL TESTO TITOLO 1 -->
                        <!-- SETTING THE TITLE TEXT 1 -->
                        <property name="label" translatable="yes">Dependencies install process</property>

                        <!-- DEFINIZIONE DELL'ALLINEAMENTO DEL TESTO -->
                        <!-- SETTING THE TEXT ALLIGNMENT -->
                        <property name="justify">center</property>

                        <!-- DEFINIZIONE DELLA POSSIBILITÀ DI ANDARE A CAPO -->
                        <!-- SETTING THE POSSIBILITY TO GO TO A NEW LINE -->
                        <property name="wrap">false</property>

                        <!-- DEFINZIONE DELL'ALLINEMENATO ORIZZONTALE DEL TESTO NEL CONTENITORE -->
                        <!-- SETTING THE HORIZONTAL TEXT ALLIGNMENT INSIDE THE CONTAINER -->
                        <property name="xalign">0.5</property>

                        <!-- DEFINZIONE DELL'ALLINEMENATO VERTICALE DEL TESTO NEL CONTENITORE -->
                        <!-- SETTING THE VERTICAL TEXT ALLIGNMENT INSIDE THE CONTAINER -->
                        <property name="yalign">0.5</property>

                        <!-- POSIZIONAMENTO DEL TESTO TITOLO 1 NELLA GRIGLIA GENERALE -->
                        <!-- TITLE TEXT 1 POSITION INSIDE THE MAIN GRID -->
                        <layout>

                            <!-- POSIZIONE VERTICALE -->
                            <!-- VERTICAL POSITION -->
                            <property name="column">0</property>

                            <!-- POSIZIONE ORIZZONTALE -->
                            <!-- HORIZONTAL POSITION -->
                            <property name="row">0</property>

                        </layout>

                        <!-- INSERISCO I PARAMETRI CSS DEL TESTO TITOLO -->
                        <!-- SETTING TITLE TEXT CSS PARAMETERS -->
                        <style>
                            <class name="Function-1-Title"/>
                        </style>

                    <!-- CHIUSURA TESTO TITOLO-->
                    <!-- TITLE TEXT END -->
                    </object>

                <!-- CHIUSURA CHILD TESTO TITOLO -->
                <!-- TITLE TEXT CHILD END -->
                </child>



                <!-- APERTURA CHILD SCORREVOLE TESTO TERMINALE 1 -->
                <!-- SCROLLABLE TERMINAL TEXT 1 CHILD START -->
                <child>

                    <!-- APERTURA SCORREVOLE TESTO TERMINALE 1 -->
                    <!-- SCROLLABLE TERMINAL TEXT 1 START -->
                    <object class="GtkScrolledWindow">

                        <!-- VISIBILITÀ DELL'OGGETTO -->
                        <!-- SETTING THE OBJECT VISIBILITY -->
                        <property name="visible">False</property>

                        <!-- DEFINIZIONE DEL'ESPANSIONE ORIZZONTALE DEL CONTENITORE -->
                        <!-- SETTING THE VERTICAL EXPANSION OF THE CONTAINER -->
                        <property name="hexpand">true</property>

                        <!-- DEFINIZIONE DELL'ESPANSIONE VERTICALE DEL CONTENITORE -->
                        <!-- SETTING THE VERTICAL EXPANSION OF THE CONTAINER -->
                        <property name="vexpand">true</property>

                        <!-- INSERISCO I PARAMETRI CSS DEL TESTO TITOLO 1-->
                        <!-- SETTING TITLE TEXT 1 CSS PARAMETERS -->
                        <style>
                            <class name="Function-1-Scrollable"/>
                        </style>

                        <!-- POSIZIONAMENTO DEL TESTO TITOLO 1 NELLA GRIGLIA GENERALE -->
                        <!-- TITLE TEXT 1 POSITION INSIDE THE MAIN GRID -->
                        <layout>

                            <!-- POSIZIONE VERTICALE -->
                            <!-- VERTICAL POSITION -->
                            <property name="column">0</property>

                            <!-- POSIZIONE ORIZZONTALE -->
                            <!-- HORIZONTAL POSITION -->
                            <property name="row">1</property>

                        </layout>


                        
                                
                        <!-- APERTURA CHILD DEL VISUALIZZATORE DI TESTO -->
                        <!-- TEXT VISUALIZER CHILD START -->
                        <child>

                            <!-- APERTURA DEL VISUALIZZATORE DI TESTO -->
                            <!-- TEXT VIEWER START -->
                            <object class="GtkTextView" id="terminal_text_1">

                                <!-- VISIBILITÀ DELL'OGGETTO -->
                                <!-- SETTING THE OBJECT VISIBILITY -->
                                <property name="visible">False</property>

                                <!-- VISIBILITÀ DEL CURSORE NEL TESTO -->
                                <!-- TEXT CURSOR VISIBILITY -->
                                <property name="cursor-visible">false</property>

                                <!-- POSSIBILITÀ DI MODIFCARE IL TESTO -->
                                <!-- TEXT EDIT POSSIBILITY -->
                                <property name="editable">false</property>

                                <!-- FONT MONOSPAZIATO -->
                                <!-- MONOSPACE FONT -->
                                <property name="monospace">true</property>

                                <!-- CRITERIO PER ANDARE A CAPO -->
                                <!-- SETTING THE SENTENCE TRUNCATION'S CRITERIA -->
                                <property name="wrap-mode">word</property>
                                
                                
                                <!-- INSERISCO I PARAMETRI CSS DEL VISUALIZZATORE DI TESTO-->
                                <!-- SETTING TEXT VIEWER CSS PARAMETERS -->
                                <style>
                                    <class name="Function-1-Textview"/>
                                </style>

                            <!-- CHIUSURA DEL VISUALIZZATORE DI TESTO -->
                            <!-- TEXT VIEWER END -->
                            </object>

                        <!-- CHIUSURA CHILD TESTO PROVA -->
                        <!-- PROVA TEXT 1 CHILD END -->
                        </child>



                    <!-- CHIUSURA SCORREVOLE TESTO TERMINALE 1 -->
                    <!-- SCROLLABLE TERMINAL TEXT 1 END -->
                    </object>  

                <!-- CHIUSURA CHILD SCORREVOLE TESTO TERMINALE 1 -->
                <!-- SCROLLABLE TERMINAL TEXT 1 CHILD END -->
                </child>
























































                <!--  -->
                <!--  -->
                <child>
                    
                    <!--  -->
                    <!--  -->
                    <object class="GtkImage" id="function_1_status_icon">

                        <!-- POSIZIONAMENTO NELLA GRIGLIA GENERALE -->
                        <!-- POSITION INSIDE THE MAIN GRID -->
                        <layout>

                            <!-- POSIZIONE VERTICALE -->
                            <!-- VERTICAL POSITION -->
                            <property name="column">0</property>

                            <!-- POSIZIONE ORIZZONTALE -->
                            <!-- HORIZONTAL POSITION -->
                            <property name="row">2</property>

                        </layout>

                    <!--  -->
                    <!--  -->
                    </object>
                
                <!--  -->
                <!--  -->
                </child>





                <!--  -->
                <!--  -->
                <child>
                    
                    <!--  -->
                    <!--  -->
                    <object class="GtkLabel" id="function_1_status_title">

                        <!-- POSIZIONAMENTO NELLA GRIGLIA GENERALE -->
                        <!-- POSITION INSIDE THE MAIN GRID -->
                        <layout>

                            <!-- POSIZIONE VERTICALE -->
                            <!-- VERTICAL POSITION -->
                            <property name="column">0</property>

                            <!-- POSIZIONE ORIZZONTALE -->
                            <!-- HORIZONTAL POSITION -->
                            <property name="row">3</property>

                        </layout>

                    <!--  -->
                    <!--  -->
                    </object>
                
                <!--  -->
                <!--  -->
                </child>





                <!--  -->
                <!--  -->
                <child>
                    
                    <!--  -->
                    <!--  -->
                    <object class="GtkLabel" id="function_1_status_subtitle">

                        <!-- POSIZIONAMENTO NELLA GRIGLIA GENERALE -->
                        <!-- POSITION INSIDE THE MAIN GRID -->
                        <layout>

                            <!-- POSIZIONE VERTICALE -->
                            <!-- VERTICAL POSITION -->
                            <property name="column">0</property>

                            <!-- POSIZIONE ORIZZONTALE -->
                            <!-- HORIZONTAL POSITION -->
                            <property name="row">4</property>

                        </layout>

                    <!--  -->
                    <!--  -->
                    </object>
                
                <!--  -->
                <!--  -->
                </child>





                <!--  -->
                <!--  -->
                <child>
                    
                    <!--  -->
                    <!--  -->
                    <object class="GtkBox" id="function_1_log_button_box">

                        <!-- POSIZIONAMENTO NELLA GRIGLIA GENERALE -->
                        <!-- POSITION INSIDE THE MAIN GRID -->
                        <layout>

                            <!-- POSIZIONE VERTICALE -->
                            <!-- VERTICAL POSITION -->
                            <property name="column">0</property>

                            <!-- POSIZIONE ORIZZONTALE -->
                            <!-- HORIZONTAL POSITION -->
                            <property name="row">5</property>

                        </layout>

                    <!--  -->
                    <!--  -->
                    </object>
                
                <!--  -->
                <!--  -->
                </child>










































































                <!-- APERTURA CHILD BOX BOTTONE DI USCITA -->
                <!-- EXIT BUTTON BOX CHILD START -->
                <child>

                    <!-- APERTURA BOX BOTTONE DI USCITA-->
                    <!-- EXIT BUTTON BOX START -->
                    <object class="GtkBox" id="function_1_exit_button_box">

                    <!-- IMPOSTAZIONE DELL'ORIENTAMENTO DEL BOX -->
                    <!-- SETTING THE BOX ORIENTATION -->
                    <property name="orientation">vertical</property>

                    <!-- IMPOSTAZIONE DELL'ALLINEAMENTO VERTICALE-->
                    <!-- SETTING THE BOX VERTICAL ALIGN -->
                    <property name="valign">center</property>

                    <!-- IMPOSTAZIONE DELL'ALLINEAMENTO ORIZZONTALE-->
                    <!-- SETTING THE BOX HORIZONTAL ALIGN -->
                    <property name="halign">center</property>

                    <!-- POSIZIONAMENTO DEL TESTO INFO 1 NELLA GRIGLIA GENERALE -->
                    <!-- INFO TEXT 1 POSITION INSIDE THE MAIN GRID -->
                    <layout>

                        <!-- POSIZIONE VERTICALE -->
                        <!-- VERTICAL POSITION -->
                        <property name="column">0</property>

                        <!-- POSIZIONE ORIZZONTALE -->
                        <!-- HORIZONTAL POSITION -->
                        <property name="row">10</property>

                    </layout>

                    <!-- INSERISCO I PARAMETRI CSS DEL BOX BOTTONE DI USCITA -->
                    <!-- SETTING EXIT BUTTON BOX CSS PARAMETERS  -->
                    <style>
                        <class name="Function-1-Exit-Button-Box"/>
                    </style>


                    <!-- INSERIMENTO CHILD BOTTONE OK 1 -->
                    <!-- OK BUTTON 1 CHILD START -->
                    <child>

                        <!-- INSERIMENTO BOTTONE OK 1 -->
                        <!-- OK BUTTON 1 START -->
                        <object class="GtkButton" id="exit_button_1">

                        <!-- PROPRIETÀ DEL BOTTONE DI USCITA -->
                        <!-- EXIT BUTTON PARAMETERS -->

                        <!-- IMPOSTAZIONE DELL'ETICHETTA DEL BOTTONE DI USCITA-->
                        <!-- EXIT BUTTON TEXT LABEL -->
                        <property name="label" translatable="yes">Exit</property>

                        <!-- INSERISCO I PARAMETRI CSS DEL BOTTONE USCITA-->
                        <!-- SETTING EXIT BUTTON CSS PARAMETERS  -->
                        <style>
                            <class name="Exit-Button-1"/>
                        </style>

                        <!-- CHIUSURA BOTTONE DI USCITA -->
                        <!-- OK BUTTON 1 END -->
                        </object>

                    <!-- CHIUSURA CHILD BOTTONE DI USCITA-->
                    <!-- EXIT BUTTON CHILD END -->
                    </child>


                    <!-- CHIUSURA BOX BOTTONE DI USCITA OK-->
                    <!-- EXIT BUTTON BOX END -->
                    </object>

                <!-- CHIUSURA CHILD BOX BOTTONE DI USCITA -->
                <!-- EXIT BUTTON BOX CHILD END -->
                </child>






            <!-- CHIUSURA GRIGLIA PRINCIPALE -->
            <!-- MAIN GRID END -->
            </object>

        <!-- CHIUSURA CHILD GRIGLIA PRINCIPALE -->
        <!-- MAIN GRID CHILD END -->
        </child>



   	<!-- CHIUSURA FINESTRA PRINCIPALE -->
	<!-- MAIN WINDOW END -->
    </object>

<!-- CHIUSURA INTERFACCIA -->
<!-- INTERFACE END -->
</interface>
